# n8n Integration Configuration
# Copy this file to .env.local and update with your n8n instance URL

# n8n Webhook Base URL
NEXT_PUBLIC_N8N_WEBHOOK_URL=http://localhost:5678/webhook

# Optional: n8n API Key (if using authentication)
# N8N_API_KEY=your_api_key_here

# Optional: Enable demo mode with mock data (true/false)
# NEXT_PUBLIC_DEMO_MODE=true
